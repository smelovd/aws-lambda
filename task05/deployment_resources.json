{
  "lambda-basic-execution": {
    "policy_content": {
      "Statement": [
        {
          "Action": [
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:PutLogEvents",
            "dynamodb:GetItem",
            "dynamodb:Query",
            "dynamodb:PutItem",
            "dynamodb:Batch*",
            "dynamodb:DeleteItem",
            "ssm:PutParameter",
            "ssm:GetParameter",
            "kms:Decrypt",
            "dynamodb:DestribeTable",
            "dynamodb:PutItem"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "resource_type": "iam_policy",
    "tags": {}
  },
  "task5_api": {
    "resource_type": "api_gateway",
    "deploy_stage": "api",
    "dependencies": [
      {
        "resource_name": "api_handler",
        "resource_type": "lambda"
      }
    ],
    "resources": {
      "/": {
        "POST": {
          "authorization_type": "NONE",
          "method_request_parameters": {
            "method.request.querystring.name": false
          },
          "default_error_pattern": true,
          "lambda_name": "api_handler",
          "integration_type": "lambda",
          "enable_proxy" : true
        }
      },
      "/events": {
        "POST": {
          "authorization_type": "NONE",
          "method_request_parameters": {
            "method.request.querystring.name": false
          },
          "default_error_pattern": true,
          "lambda_name": "api_handler",
          "integration_type": "lambda",
          "enable_proxy" : true
        }
      }
    },
    "minimum_compression_size": 0,
    "tags": {}
  },
  "Events": {
    "resource_type": "dynamodb_table",
    "hash_key_name": "id",
    "hash_key_type": "N",
    "sort_key_type": "S",
    "read_capacity": 1,
    "write_capacity": 1,
    "global_indexes": [],
    "autoscaling": [],
    "tags": {}
  }
}